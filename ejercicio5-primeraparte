package ejercicio5;
import java.util.Scanner;

public class Ejercicio5 {
	public static void main (String []args){
		Scanner teclado=new Scanner(System.in);
		
		System.out.println("Ingrese el angulo en readianes : ");
		float radian=teclado.nextFloat();
		float sexagesimal=0.0f;
		
		sexagesimal=(float)(radian*180/Math.PI);
		
		System.out.println("El valor en grados sexagesimales es: "+sexagesimal);
	}

}
