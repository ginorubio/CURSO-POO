
package ejercicio5;
import java.util.Scanner;

public class Ejercicio5 {
    public static void main (String []args){
        Scanner teclado=new Scanner(System.in);
        
        System.out.println("Ingresar el valor de m");
        int m=teclado.nextInt();
        System.out.println("Ingresar el valor de n");
        int n=teclado.nextInt();
        System.out.println("Ingresar el valor de q");
        int q=teclado.nextInt();
        
        if((m%n==0)&& (m%q==0))
            System.out.println("m es multiplo de n y q");
        else 
            System.out.println("m  No es multiplo de n y q");
    }
}
